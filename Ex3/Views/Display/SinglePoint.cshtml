
@{
    ViewBag.Title = "SinglePoint";
}


<div style="display:none;">
    <img id="map" ++++
         src="~/Content/export-map-share.png" />
</div>
<!img id="bg" src="~/Content/export-map-share.png" alt="World Map">
<canvas id="myCanvas"></canvas>

<script src="~/DisplayPoint.js" language="javascript" type="text/javascript"></script>


<script>
    setup("myCanvas");
    $.post("@Url.Action("GetLocation", "Display")").done(function (xml) {


        var coordinates = xml.split(',');
        drawLineWithPoint(parseFloat(coordinates[0]), parseFloat(coordinates[1]), -1, -1, "myCanvas");

    });
</script>
